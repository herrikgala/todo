{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "useTabs": true,
  "tabWidth": 1,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "vueIndentScriptAndStyle": false,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "auto",
  "vueStrictStyle": true,
  "vueSingleFileComponentIndentation": true
}
